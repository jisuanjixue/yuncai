{
  "name": "yuncaiyitong",
  "version": "1.0.0",
  "private": true,
  "description": "葱易通小程序",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp --open",
    "build:weapp:upload": "taro build --type weapp --upload",
    "build:weapp:preview": "taro build --type weapp --preview",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "NODE_ENV=production taro build --type weapp --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prepare": "husky install"
  },
  "taroConfig": {
    "version": "1.0.0",
    "desc": "上传描述"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "{package.json,.eslintrc.js,.prettierrc.js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@babel/runtime": "^7.7.7",
    "@hookform/devtools": "^4.0.1",
    "@tarojs/cli": "^3.4.8",
    "@tarojs/components": "3.4.8",
    "@tarojs/plugin-framework-react": "^3.4.8",
    "@tarojs/plugin-html": "3.4.8",
    "@tarojs/react": "3.4.8",
    "@tarojs/redux": "^2.2.10",
    "@tarojs/redux-h5": "^2.2.10",
    "@tarojs/rn-runner": "^3.4.8",
    "@tarojs/runtime": "3.4.8",
    "@tarojs/taro": "3.4.8",
    "@tarojs/taro-rn": "^3.4.8",
    "antd-mobile": "^2.3.4",
    "dayjs": "^1.8.29",
    "echarts-taro3-react": "^1.0.13",
    "global": "^4.4.0",
    "graphql": "^16.0.1",
    "install": "^0.13.0",
    "preact": "^10.6.4",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.2",
    "react-native": "^0.66.0",
    "react-redux": "^7.2.5",
    "react-use": "^17.3.1",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-apollo": "^1.2.5",
    "taro-virtual-list": "^1.0.12",
    "tarojs-router-next": "^2.5.3",
    "wx-apollo-fetcher": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.4.8",
    "@tarojs/plugin-react-devtools": "^3.3.7",
    "@tarojs/webpack-runner": "3.4.8",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-taro": "3.4.8",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react-devtools": "^4.18.0",
    "stylelint": "9.3.0",
    "tailwind-taro": "^0.0.1",
    "tailwindcss": "^2.2.15",
    "tarojs-router-next-plugin": "^2.5.3",
    "typescript": "^4.1.0",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
